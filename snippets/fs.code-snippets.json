    "fs input reference image": {
      "prefix": "<cms-ref-image, cms-ref-image, <cms-image, cms-image, image",
      "body": [
        "<FS_REFERENCE name=\"${1|st_image|}\" imagePreview=\"${2|yes,no|}\" upload=\"${3|yes,no|}\">",
        "  <FILTER>",
        "    <ALLOW type=\"${4|PICTURE,ALL,CONTENT2,CONTENTFOLDER,DOCUMENTGROUP,FILE,FOLDERS,FORMATTEMPLATE,FORMATTEMPLATEFOLDER,GCAFOLDER,GCAPAGE,LINKTEMPLATE,MEDIA,MEDIAFOLDER,PAGE,PAGEFOLDER,PAGEREF,PAGEREFFOLDER,SCHEMA,SCRIPT,SECTIONTEMPLATE,SITESTOREFOLDER,STYLETEMPLATE,TABLEFORMATTEMPLATE,TEMPLATE,TEMPLATEFOLDER,WORKFLOW|}\"/>",
