        "   </FILTER>",
        "   <LANGINFOS>",
        "     <LANGINFO lang=\"*\" label=\"${6|Image|}\" description=\"${7|Please select a picture|}\"/>",
        "    </LANGINFOS>",
        "    <PROJECTS>",
        "      <LOCAL name=\".\">",
        "         <SOURCES>",
        "           <FOLDER name=\"${8|root|}\" store=\"${9|mediastore|}\"/>",
        "        </SOURCES>",
        "      </LOCAL>",
        "    </PROJECTS>",
        "</FS_REFERENCE>"
      ],
      "description": "fs input reference image"
    },
